{
  "name": "humane-discovery",
  "version": "1.1.8",
  "description": "Humane Discovery",
  "repository": "git@github.com:360fy/humane-discovery.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/ && webpack --progress --colors",
    "prepublish": "npm run compile"
  },
  "bin": {
    "humane-cli": "./cli.js",
    "humane-server": "./server.js"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "agentkeepalive": "2.0.5",
    "babel-polyfill": "6.7.4",
    "bluebird": "3.3.4",
    "command-line-boilerplate": "1.1.3",
    "config-boilerplate": "1.1.2",
    "events": "1.1.0",
    "expressjs-boilerplate": "1.1.4",
    "humane-cockpit": "1.1.2",
    "humane-data-pipeline": "1.1.10",
    "humane-indexer": "1.1.8",
    "humane-searcher": "1.1.6",
    "immutable": "3.7.6",
    "lodash": "4.6.1",
    "md5": "2.1.0",
    "mkdirp": "0.5.1",
    "moment": "2.12.0",
    "request": "2.69.0"
  },
  "devDependencies": {
    "babel-core": "6.7.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-node5": "11.0.1",
    "babel-preset-react": "6.5.0",
    "eslint": "2.5.3",
    "eslint-config-360fy": "1.1.2",
    "eslint-plugin-react": "4.2.3",
    "webpack-builder-boilerplate": "1.1.2"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
